
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessedEvent)
    if gameProcessedEvent then
        return 
    end
   if not game.Players.LocalPlayer.Character:FindFirstChild("Kira") then return end



    if input.KeyCode == Enum.KeyCode.One then
game:GetService("ReplicatedStorage"):WaitForChild("Characters"):WaitForChild("Kira"):WaitForChild("Remotes"):WaitForChild("MoveOne"):FireServer()
elseif input.KeyCode == Enum.KeyCode.Two then
game:GetService("ReplicatedStorage"):WaitForChild("Characters"):WaitForChild("Kira"):WaitForChild("Remotes"):WaitForChild("MoveTwo"):FireServer()
elseif input.KeyCode == Enum.KeyCode.Three then
  game.Players.LocalPlayer.Character.Kira.DestroyCounter:FireServer()
game:GetService("ReplicatedStorage"):WaitForChild("Characters"):WaitForChild("Kira"):WaitForChild("Remotes"):WaitForChild("Counter"):FireServer()
elseif input.KeyCode == Enum.KeyCode.Four then
game:GetService("ReplicatedStorage"):WaitForChild("Characters"):WaitForChild("Kira"):WaitForChild("Remotes"):WaitForChild("MoveFour"):FireServer()
elseif input.KeyCode == Enum.KeyCode.Five then
game:GetService("ReplicatedStorage"):WaitForChild("Characters"):WaitForChild("Kira"):WaitForChild("Remotes"):WaitForChild("MoveFive"):FireServer()
elseif input.KeyCode == Enum.KeyCode.F then

elseif input.KeyCode == Enum.KeyCode.E then
game:GetService("ReplicatedStorage"):WaitForChild("Characters"):WaitForChild("Kira"):WaitForChild("Remotes"):WaitForChild("MoveE"):FireServer()
elseif input.KeyCode == Enum.KeyCode.H then
for i=1,100 do
game:GetService("ReplicatedStorage"):WaitForChild("Characters"):WaitForChild("Kira"):WaitForChild("Remotes"):WaitForChild("MoveOne"):FireServer()
end
   end
end)
