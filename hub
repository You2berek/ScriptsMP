-- Fluent port of your Rayfield UI (based on the doc you provided)
-- Uses Fluent as in: https://github.com/dawid-scripts/Fluent (example usage in your uploaded file). :contentReference[oaicite:1]{index=1}
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

-- music and helper (copied logic from original)
local function MUSIC()
    local oldMusic = workspace:FindFirstChild("GameMusic")
    if oldMusic then oldMusic:Destroy() end

    local music = Instance.new("Sound")
    music.Name = "GameMusic"
    music.Volume = 0
    music.Looped = false
    music.Parent = workspace

    local trackIds = {
        "rbxassetid://1846368080",
        "rbxassetid://9038252619",
        "rbxassetid://9046476113",
        "rbxassetid://1843584386",
        "rbxassetid://1838499484",
        "rbxassetid://1847863129",
        "rbxassetid://9038253134",
        "rbxassetid://1838561243",
        "rbxassetid://1839822906"
    }

    local playedTracks = {}
    local lastTrack = nil

    local function getNextTrack()
        if #playedTracks >= #trackIds then
            playedTracks = {}
        end
        local track
        repeat
            track = trackIds[math.random(1, #trackIds)]
        until (not table.find(playedTracks, track)) and (track ~= lastTrack)
        table.insert(playedTracks, track)
        lastTrack = track
        return track
    end

    local function playNext()
        local nextId = getNextTrack()
        music.SoundId = nextId
        music:Play()
    end

    music.Ended:Connect(playNext)
    playNext()
end
task.spawn(MUSIC)

-- helper to fire change char (keeps same remote signature)
local function fireChangeChar(characterName, starterSelectScript, skin)
    if skin and skin ~= 0 then
        ReplicatedStorage.SelectACharacter.ChangeChar:FireServer(characterName, starterSelectScript, skin)
    else
        ReplicatedStorage.SelectACharacter.ChangeChar:FireServer(characterName, starterSelectScript)
    end
end

-- Grab the Menu gui objects like original
local success, MenuGui = pcall(function()
    return LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Menu")
end)
if not success or not MenuGui then
    warn("Menu GUI not found; script expects Menu under PlayerGui (as in original).")
end

local StarterSelectScript
pcall(function()
    StarterSelectScript = StarterGui:WaitForChild("Menu").Frame.TopMiddle.Select:WaitForChild("SelectScript")
end)

local CharButtonsFolder
pcall(function()
    CharButtonsFolder = MenuGui.Frame.Placement.CharButtons
end)

-- load Fluent like your example
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

-- Create window (keeps theme and size like example)
local Window = Fluent:CreateWindow({
    Title = "Marvellous Playground",
    SubTitle = "by Finger, Doakes, Lundy, Hector and Dexter",
    TabWidth = 160,
    Size = UDim2.fromOffset(600, 450),
    Acrylic = false,
    Theme = "Amethyst",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    Characters = Window:AddTab({ Title = "Characters", Icon = "circle-user" }),
    Scripts = Window:AddTab({ Title = "Scripts", Icon = "sticky-note" }),
    CharScripts = Window:AddTab({ Title = "Character Scripts", Icon = "code" }),
	   Etcetera = Window:AddTab({ Title = "Misc/Fun", Icon = "code" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options

-- Save and interface manager wiring (as in your doc)
SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)
SaveManager:IgnoreThemeSettings()
SaveManager:SetFolder("FluentScriptHub/specific-game")
InterfaceManager:SetFolder("FluentScriptHub")
InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)

-- Persistent state like original
local SkinSelection = 0
local FlashSwapEnabled = false

-- CHARACTERS TAB
do
    -- Skin slider
    local sliderObj = Tabs.Characters:AddSlider("SkinSelector", {
        Title = "Skin Selector",
        Description = "0–8 (0 = no skin)",
        Default = 0,
        Min = 0,
        Max = 8,
        Rounding = 0,
        Callback = function(Value)
            SkinSelection = Value
            -- debug
            print("SkinSelection set to:", SkinSelection)
        end
    })

    -- FlashSwap toggle
    local flashToggle = Tabs.Characters:AddToggle("FlashSwap", { Title = "FlashSwap", Default = false })
    flashToggle:OnChanged(function()
        FlashSwapEnabled = flashToggle.Value
        print("FlashSwapEnabled:", FlashSwapEnabled)
    end)

    -- Create a label/paragraph explaining behavior
    Tabs.Characters:AddParagraph({
        Title = "Note",
        Content = "FlashSwap teleports your newly spawned character to your previous HRP CFrame if enabled."
    })

    -- Create character buttons dynamically from CharButtonsFolder like original
    if CharButtonsFolder then
        for _, obj in ipairs(CharButtonsFolder:GetChildren()) do
            if obj:IsA("ImageButton") then
                local charName = obj.Name
                Tabs.Characters:AddButton({
                    Title = charName,
                    Description = "Select " .. charName,
                    Callback = function()
                        -- FlashSwap behavior
                        if FlashSwapEnabled then
                            local currentChar = LocalPlayer.Character
                            local savedCFrame
                            if currentChar and currentChar:FindFirstChild("HumanoidRootPart") then
                                savedCFrame = currentChar.HumanoidRootPart.CFrame
                            end

                            local connection
                            connection = LocalPlayer.CharacterAdded:Connect(function(newChar)
                                connection:Disconnect()
                                local root = newChar:WaitForChild("HumanoidRootPart", 5)
                                if savedCFrame and root then
                                    task.wait(0.15)
                                    root.CFrame = savedCFrame
                                end
                            end)
                        end

                        -- Change character (preserve skin selection)
                        if SkinSelection ~= 0 then
                            fireChangeChar(charName, StarterSelectScript, SkinSelection)
                        else
                            fireChangeChar(charName, StarterSelectScript)
                        end
                    end
                })
            end
        end
    else
        Tabs.Characters:AddParagraph({ Title = "Warning", Content = "CharButtonsFolder not found; dynamic char buttons unavailable." })
    end
end

-- SCRIPTS TAB
do
    -- Buttons (same loadstring links as original)
    Tabs.Scripts:AddButton({
        Title = "Infinite Yield",
        Description = "Load Infinite Yield",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
        end
    })

	   Tabs.Scripts:AddButton({
        Title = "St HUB",
        Description = "Load St HUB",
        Callback = function()
            loadstring(game:HttpGet("https://gist.githubusercontent.com/RuanST1/505d0a20d7d8a09a4e51cb1080a66d35/raw/54b7b2bc5724ae36d003d5e42efefe6de0f3984e/sthub1.3"))()
        end
    })

    Tabs.Scripts:AddButton({
        Title = "Stunlock Killer (Experimental)",
        Description = "Experimental script",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/You2berek/ScriptsMP/refs/heads/main/DREDAYNIGGA"))()
        end
    })

    Tabs.Scripts:AddButton({
        Title = "Galahad's Menu 2 (not ported yet)",
        Description = "Menu (not ported)",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/You2berek/ScriptsMP/refs/heads/main/TrollMenu"))()
        end
    })

    Tabs.Scripts:AddButton({
        Title = "Invisibility (Shows Player Name)",
        Description = "Fire mini explosive",
        Callback = function()
            ReplicatedStorage.Characters['TheBatman'].Remotes.MiniExplosive:FireServer(LocalPlayer)
        end
    })

    Tabs.Scripts:AddButton({
        Title = "Speedster Addition (Tornado + SuperSpeed)",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/You2berek/ScriptsMP/refs/heads/main/Tornado"))()
        end
    })

    -- God-Mode Toggle (loop firing remote)
    local godModeEnabled = false
    local GodToggle = Tabs.Scripts:AddToggle("GodMode", { Title = "God-Mode", Default = false })
    GodToggle:OnChanged(function()
        if GodToggle.Value then
            if not godModeEnabled then
                godModeEnabled = true
                coroutine.wrap(function()
                    while godModeEnabled do
                        ReplicatedStorage.Characters.Quicksilver.Remotes.TimeStop:FireServer(LocalPlayer)
                        wait(3)
                    end
                end)()
            end
        else
            godModeEnabled = false
        end
    end)

    -- Loop-Heal toggle
    local ramboHealEnabled = false
    local HealToggle = Tabs.Scripts:AddToggle("LoopHeal", { Title = "Loop-Heal", Default = false })
    HealToggle:OnChanged(function()
        if HealToggle.Value then
            if not ramboHealEnabled then
                ramboHealEnabled = true
                coroutine.wrap(function()
                    while ramboHealEnabled do
                        ReplicatedStorage.Characters.Rambo.Remotes.WoundHeal:FireServer()
                        task.wait()
                    end
                end)()
            end
        else
            ramboHealEnabled = false
        end
    end)

    -- Remove CantAttack toggle
    local cantAttackRemoverEnabled = false
    local CantAttackToggle = Tabs.Scripts:AddToggle("CantAttackRemover", { Title = "Remove CantAttack", Default = false })
    CantAttackToggle:OnChanged(function()
        if CantAttackToggle.Value then
            if not cantAttackRemoverEnabled then
                cantAttackRemoverEnabled = true
                coroutine.wrap(function()
                    while cantAttackRemoverEnabled do
                        local char = LocalPlayer.Character
                        if char and char:FindFirstChild("CantAttack") then
                            char.CantAttack:Destroy()
                        end
                        task.wait()
                    end
                end)()
            end
        else
            cantAttackRemoverEnabled = false
        end
    end)

    -- Player dropdown and refresh + instakill button
    local function getPlayerNames()
        local names = {}
        for _, p in ipairs(Players:GetPlayers()) do
            table.insert(names, p.Name)
        end
        return names
    end

    local PlayerDropdown = Tabs.Scripts:AddDropdown("SelectPlayer", {
        Title = "Select Player",
        Values = getPlayerNames(),
        Multi = false,
        Default = 1
    })

    -- Selected player name state
    local selectedPlayer = nil
    -- Set initial dropdown value
    PlayerDropdown:SetValue(getPlayerNames()[1] or "")

    -- Provide on-change and a refresh button
    -- Some Fluent versions use :OnChanged; fallback to polling in Callback if not available
    if PlayerDropdown.OnChanged then
        PlayerDropdown:OnChanged(function(val)
            selectedPlayer = val
            print("Selected player:", selectedPlayer)
        end)
    else
        -- attempt to read Options (best-effort)
        selectedPlayer = PlayerDropdown.Value or getPlayerNames()[1]
    end

    Tabs.Scripts:AddButton({
        Title = "Refresh Player List",
        Callback = function()
            local values = getPlayerNames()
            PlayerDropdown:SetValues(values)
            PlayerDropdown:SetValue(values[1] or "")
        end
    })

    Tabs.Scripts:AddButton({
        Title = "Instakill Player (Once)",
        Callback = function()
            local name = selectedPlayer or PlayerDropdown.Value
            if name and name ~= "" then
                local target = workspace:FindFirstChild(name)
                if target then
                    print("Successfully fired, player " .. name)
                    for i = 1, 50 do
                        task.wait(0.01)
                        ReplicatedStorage.Characters.Combat.Remotes.ComboEndRemote:FireServer(target)
                    end
                else
                    warn("Player instance not found in workspace:", name)
                end
            else
                warn("No player selected!")
            end
        end
    })
end
do
    Tabs.Etcetera:AddParagraph({ Title = "Miscellaneous", Content = "Misc/Fun below"})

      Tabs.Etcetera:AddButton({ Title = "Remove Ragdoll (SKY SQUID GAME)", Callback = function()
        if workspace.Map:FindFirstChild("Ragdoller") then
workspace.Map.Ragdoller:Destroy()
workspace.Map.HitGround:Destroy()
end
    end})

end
