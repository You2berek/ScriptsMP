function RollForwardDash(arg1) -- Line 37
    local BodyPosition = Instance.new("BodyPosition")
    BodyPosition.MaxForce = Vector3.new(1000000, 0, 1000000)
    BodyPosition.P = 100000
    BodyPosition.D = 3000
